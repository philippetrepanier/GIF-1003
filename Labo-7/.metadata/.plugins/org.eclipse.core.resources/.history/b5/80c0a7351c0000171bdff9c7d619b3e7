#include "TicTacToe.h"

using namespace std;

namespace labo
{

TicTacToe::TicTacToe()
{
	for (int i = 0; i < TicTacToe::TAILLE_PLATEAU; ++i)
	{
		for (int j = 0; j < TicTacToe::TAILLE_PLATEAU; ++j)
		{
			m_plateau[i][j] = ' ';
		}
	}

}

ostream& operator<<(ostream& os, const TicTacToe t)
{
	os << t.reqPlateauFormate();
	return os;
}

string TicTacToe::reqPlateauFormate() const
{
}

} /* namespace labo */
