 * \brief Programme d�mo calcul de x � la puissance y en langage C++
 */
#include <iostream>
using namespace std;
int main(void) {
	int x = 100;
	int y = 6;
	int i = 0;
	int puissance;
	puissance = x;
	while (i < y - 1) {
		puissance = puissance * x;
		i++;
	}
	return 0;
}

