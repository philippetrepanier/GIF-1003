#include <iostream>
using namespace std;
int main(void) {
	int n = 5, p = 9;
	int q;
	float x;

	q = n < p;
	q = n == p;
	q = n % n + p > n;
	x = p / n;
	x = (float) p / n;
	x = (p + 0.5) / n;
	x = (int) (p + 0.5) / n;
	q = n * (p > n ? n : p);
	q = n * (p < n ? n : p);
	return 0;
}

