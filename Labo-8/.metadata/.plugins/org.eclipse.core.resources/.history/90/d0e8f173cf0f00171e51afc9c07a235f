#include "Sphere.h"
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	double rayon = 5;
	labo8::Sphere spheretest(rayon);
	double volume = spheretest.reqVolume();
	cout << volume << endl;
	cout << 4.0 / 3.0 * M_PI * pow(rayon, 3) << endl;
	return 0;
}
