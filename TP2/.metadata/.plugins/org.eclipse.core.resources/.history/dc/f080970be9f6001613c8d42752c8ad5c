int main()
{
	char ligne = 240;
	cout << "Bienvenue Ã  l'outil d'ajout d'une personne!" << endl;
	cout << ligne << endl;

	tp::Personne julie("Bou", "Julie", util::Date(18, 5, 1997), "418 658-2888");
