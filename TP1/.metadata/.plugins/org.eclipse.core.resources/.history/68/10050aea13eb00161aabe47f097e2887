#include "validationFormat.h"

using namespace std;

bool validerTelephone(const string& p_telephone)
{
	for (string::const_iterator lettre = p_telephone.begin(), end = p_telephone.end(); lettre != end; ++lettre)
	{
		break;
	}

	return false;
}

