#include "validationFormat.h"
#include <iostream>
#include <typeinfo>

using namespace std;

bool validerTelephone(const string& p_telephone)
{
	for (string::const_iterator lettre = p_telephone.begin(), end = p_telephone.end(); lettre != end; ++lettre, ++i)
	{
		cout << "Un char: " << *lettre << endl;
		if (*lettre == ' ')
		{
			cout << "ESPACE" << endl;
		}
	}

	return false;
}

