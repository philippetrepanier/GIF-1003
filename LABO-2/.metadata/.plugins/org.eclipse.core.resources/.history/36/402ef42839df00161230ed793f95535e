#include <iostream>
using namespace std;

int main() {
	int nombrelignes;
	cout << "Veuillez entrer un nombre: " << endl;
	cin >> nombrelignes;

	for (int i = 1; i <= nombrelignes; i++) {
		cout << "Nombre: " << i << endl;

	}

}

